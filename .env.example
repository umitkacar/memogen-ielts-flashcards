# ========================================
# MEMO-IMAGEN Environment Variables
# ========================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# ========================================
# OpenAI Configuration
# ========================================
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_ORG_ID=org-xxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_MODEL=gpt-4o
OPENAI_VISION_MODEL=gpt-4o

# ========================================
# Anthropic Claude Configuration
# ========================================
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# ========================================
# Google Gemini Configuration
# ========================================
GOOGLE_API_KEY=AIzaxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GEMINI_MODEL=gemini-2.0-flash-exp

# ========================================
# Stability AI (Stable Diffusion)
# ========================================
STABILITY_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
STABILITY_HOST=https://api.stability.ai

# ========================================
# Hugging Face Configuration
# ========================================
HUGGINGFACE_API_KEY=hf_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
HUGGINGFACE_MODEL=stabilityai/stable-diffusion-xl-base-1.0

# ========================================
# AWS Configuration
# ========================================
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX
AWS_SECRET_ACCESS_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AWS_REGION=us-east-1
AWS_S3_BUCKET=memo-imagen-assets

# ========================================
# Firebase Configuration
# ========================================
FIREBASE_API_KEY=AIzaxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
FIREBASE_AUTH_DOMAIN=memo-imagen.firebaseapp.com
FIREBASE_PROJECT_ID=memo-imagen
FIREBASE_STORAGE_BUCKET=memo-imagen.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:web:xxxxxxxxxxxxxxxx
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# ========================================
# Database Configuration
# ========================================
DATABASE_URL=postgresql://user:password@localhost:5432/memo_imagen
REDIS_URL=redis://localhost:6379

# ========================================
# Vector Database (Pinecone)
# ========================================
PINECONE_API_KEY=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
PINECONE_ENVIRONMENT=us-east-1-aws
PINECONE_INDEX=memo-imagen

# ========================================
# Vector Database (Weaviate) - Alternative
# ========================================
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ========================================
# Application Configuration
# ========================================
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000/api

# ========================================
# React Native / Expo Configuration
# ========================================
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_ENV=development

# ========================================
# External APIs (Optional)
# ========================================
# Midjourney (Unofficial API)
MIDJOURNEY_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ElevenLabs (Voice)
ELEVENLABS_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Replicate (AI Models)
REPLICATE_API_TOKEN=r8_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ========================================
# Monitoring & Analytics (Optional)
# ========================================
SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/xxxxxxx
MIXPANEL_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ANALYTICS_WRITE_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ========================================
# Development & Testing
# ========================================
DEBUG=true
LOG_LEVEL=debug
ENABLE_MOCK_API=false

# ========================================
# Rate Limiting & Quotas
# ========================================
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# ========================================
# Security
# ========================================
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
ENCRYPTION_KEY=your-32-character-encryption-key
SESSION_SECRET=your-session-secret-key

# ========================================
# Feature Flags
# ========================================
FEATURE_IMAGE_GENERATION=true
FEATURE_VOICE_SYNTHESIS=false
FEATURE_AR_MODE=false
FEATURE_COLLABORATIVE_LEARNING=false

# ========================================
# Model Configuration
# ========================================
MAX_TOKENS=2000
TEMPERATURE=0.7
TOP_P=1.0
FREQUENCY_PENALTY=0.0
PRESENCE_PENALTY=0.0

# Image Generation Settings
IMAGE_WIDTH=1024
IMAGE_HEIGHT=1024
IMAGE_QUALITY=standard
IMAGE_STYLE=natural

# ========================================
# Cache Configuration
# ========================================
CACHE_TTL=3600
ENABLE_CACHE=true

# ========================================
# Localization
# ========================================
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,tr,es,fr,de,ja,zh
